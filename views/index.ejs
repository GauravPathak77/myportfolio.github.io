    <script>
      let str = ["I","'","m"," ","G","a","u","r","a","v"];
      let headStr = ["a"," ","p","r","o","g","r","a","m","m","e","r,"," ","d","e","v","l","o","p","e","r,",".",".",".",".","."];
      object1 = document.querySelector(".VAL");
      object2 = document.querySelector(".head2");
      object = object1;
      let value = "";
      console.log("Working");
      let i=0,c=0,idx=0,c2=headStr.length;
      setInterval(() => {
        if(idx>=headStr.length && c>=str.length){
          value = "";
          i=0;
          c=0;
          object2.innerHTML = "";
        }
        if(i>=str.length && c2>=headStr.length){
          value = "";
          idx = 0;
          c2 = 0;
        }
        
        if(c>=str.length) {
            headText();
        }
        else{
            Text();
        }
}, 1000);
      function Text(){
          object1.innerHTML = value + str[i++];
           value =  object1.innerHTML;
           c++;
        }
        function headText(){
          object2.innerHTML = value + headStr[idx++];
           value =  object2.innerHTML;
           c2++;
        }
    </script>




